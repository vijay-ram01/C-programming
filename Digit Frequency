#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main()
 {

    char s[1000];
    gets(s);
    int c=0;
    for(int j=0;j<=9;j++)
    {
        for(int i=0;s[i]!='\0';i++)
        {
            if(j==(s[i]-'0'))
            {
                c++;
            }
        }
        printf("%d ",c);
        c=0;

    }    
    return 0;
}
