#include<stdio.h>
int sum();
int main()
{
    int a,b,c,d;
    scanf("%d %d %d %d",&a,&b,&c,&d);
    printf("%d",sum(a,b,c,d));
    return 0;
}

int sum(int a1,int a2,int a3,int a4)
{
    if(a1>a2 && a1>a3 && a1>a4)
    {
        return a1;
    }
    if(a2>a1 && a2>a3 && a2>a4)
    {
        return a2;
    }
    if(a3>a1 && a3>a2 && a3>a4)
    {
        return a3;
    }
    if(a4>a1 && a4>a2 && a4>a3)
    {
        return a4;
    }
    return 0;
}
